<Window x:Class="RandomAsyncNumberGeneration.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RandomAsyncNumberGeneration"
        mc:Ignorable="d"
        Title="Random Async Number Generation" Height="350" Width="525">
    <Grid>
		<Button x:Name="cancelGeneration" Content="Cancel" HorizontalAlignment="Left" Margin="90,10,0,0" VerticalAlignment="Top" Width="75" Click="cancelGeneration_Click"/>
		<ListBox x:Name="numbers" HorizontalAlignment="Left" Height="267" Margin="10,42,0,0" VerticalAlignment="Top" Width="497"/>
		<Button x:Name="start" Content="Start" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75" Click="start_Click"/>

	</Grid>
</Window>
